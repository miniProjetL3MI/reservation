<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E8E8F9">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="44dp"
                    android:layout_height="89dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="75dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/btn_arraw2" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="wrap_content"
                    android:layout_height="228dp"
                    android:scaleType="fitEnd"
                    app:srcCompat="@drawable/top_tight" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:text="Formulaire Medecin"
                android:textColor="#6677FF"
                android:textSize="30sp"
                android:textStyle="bold"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/editTextTextNom"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edittext_background"
                android:drawableStart="@drawable/name"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="Nom"
                android:inputType="textPersonName"
                android:textColorHint="#756C6C"
                android:textSize="18sp"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/editTextTextPrenom"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edittext_background"
                android:drawableStart="@drawable/name"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="Prénom"
                android:inputType="textPersonName"
                android:textColorHint="#756C6C"
                android:textSize="18sp"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/editTextTextNumTel"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edittext_background"
                android:drawableStart="@drawable/telephonenew"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="Numéro de téléphone"
                android:inputType="phone"
                android:textColorHint="#756C6C"
                android:textSize="18sp"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/editTextTextAdresse"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edittext_background"
                android:drawableStart="@drawable/adresse"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="Adresse"
                android:inputType="textPostalAddress"
                android:textColorHint="#756C6C"
                android:textSize="18sp"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/editTextTextSpecialite"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edittext_background"
                android:drawableStart="@drawable/specialite"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="Spécialité"
                android:inputType="text"
                android:textColorHint="#756C6C"
                android:textSize="18sp"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/editTextTextHeureOuverture"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edittext_background"
                android:drawableStart="@drawable/heure_ouverture"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="Heure d'ouverture"
                android:inputType="text"
                android:textColorHint="#756C6C"
                android:textSize="18sp"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/editTextTextHeureFermeture"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edittext_background"
                android:drawableStart="@drawable/heure_fermeture"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="Heure de fermeture"
                android:inputType="text"
                android:textColorHint="#756C6C"
                android:textSize="18sp"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextHeureOuverture"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/editTextTextJourTravail"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edittext_background"
                android:drawableStart="@drawable/datenaiss"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="Jour de travail"
                android:inputType="text"
                android:textColorHint="#756C6C"
                android:textSize="18sp"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextHeureFermeture"
                tools:ignore="MissingConstraints" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView16"
                    android:layout_width="196dp"
                    android:layout_height="183dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/bottom_left"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="-10dp" />
                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="103dp"
                    android:layout_height="92dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="70dp"
                    app:srcCompat="@drawable/btn_arraw1"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="276dp"
                    tools:layout_editor_absoluteY="737dp" />


            </LinearLayout>


        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
